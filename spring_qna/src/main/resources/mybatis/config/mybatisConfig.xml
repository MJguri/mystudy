  	<typeAlias type="mj.guri.vo.QnaVO" alias="qnaVO"/>
  	<typeAlias type="mj.guri.vo.AdminQnaVO" alias="adminQnaVO"/>
  	<typeAlias type="mj.guri.vo.QnaDetailVO" alias="qnaDetailVO"/>
